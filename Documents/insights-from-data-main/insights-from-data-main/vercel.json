{
  "headers": [
    {
      "source": "/curriculo.pdf",
      "headers": [
        {
          "key": "Content-Type",
          "value": "application/pdf"
        },
        {
          "key": "Cache-Control",
          "value": "public, max-age=31536000, immutable"
        }
      ]
    }
  ],
  "rewrites": [
    {
      "source": "/((?!.*\\.[a-z]{2,4}$).*)",
      "destination": "/index.html"
    }
  ]
}

